<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>

    <record id="effectivity_date_wizard_view" model="ir.ui.view">
      <field name="name">effectivity.date.wizard.from</field>
      <field name="model">effectivity.date.wizard</field>
      <field name="arch" type="xml">
        <form string="Allowance's Effectivity Date">
          <sheet>
            <group>
              <field name="effectivity_date"/>
            </group>
          </sheet>
          <footer>
            <button name="submit_date" string="Submit" type="object" class="oe_highlight"/>
            <button string="Cancel" class="oe_link" special="cancel"/>
          </footer>
        </form>
      </field>
    </record>

  </data>
</odoo>
